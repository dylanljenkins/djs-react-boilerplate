{
  "name": "dj-react-boilerplate",
  "version": "0.0.1",
  "description": "DJ's React Boilerplate",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback",
    "build": "webpack -p --progress --colors",
    "buildServer": "webpack --config webpack/webpack.server.config.js",
    "runServer": "cd build && node server.js",
    "buildAll": "npm run build && npm run buildServer",
    "buildAllAndRun": "npm run build && npm run buildServer && npm run runServer"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "reactstrap": "^4.8.0",
    "bootstrap": "^4.0.0-beta"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/node": "^8.0.30",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "autoprefixer": "^7.1.4",
    "awesome-typescript-loader": "^3.2.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "typings-for-css-modules-loader": "^1.6.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "author": "Dylan Jenkins",
